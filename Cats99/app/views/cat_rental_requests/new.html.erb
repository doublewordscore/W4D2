<h1> Request Cat Rental </h1>

<form action="<%= cat_rental_requests_url %>" method="POST">


  <select id="cat" name="cat_rental_request[cat_id]">
  <option value = "select a cat" disable="true" selected="true">Select a cat.</option>
  <% Cat.all.each do |cat| %>
    <option value="<%= cat.id %>"><%= cat.name %></option>
  <% end %>
  </select>

  <input type="date" name="cat_rental_request[start_date]">
  <input type="date" name="cat_rental_request[end_date]">

  <input type="submit" value="submit">

</form>
